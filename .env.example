# ATLAS Environment Variables Template
# -------------------------------------
# Copy this file to .env and fill in your actual values

# === NEO4J (Graph Database) ===
NEO4J_URI=neo4j+s://your-instance.databases.neo4j.io
NEO4J_USER=neo4j
NEO4J_PASSWORD=your_password

# === GROQ API Keys ===
GROQ_API_KEY=gsk_xxxxxxxxxxxxx
GROQ_API_KEY_BACKUP=
GROQ_API_KEY_3=
GROQ_API_KEY_4=

# === GEMINI API Keys ===
GEMINI_API_KEY=AIzaSyxxxxxxxxxxxxx
GEMINI_API_KEY_2=
GEMINI_API_KEY_3=

# === FAZ-Y: Qdrant Cloud (Vector Database) ===
# Get from: https://cloud.qdrant.io
QDRANT_URL=https://xxxxx.eu-central-1.aws.cloud.qdrant.io
QDRANT_API_KEY=qdrt_xxxxxxxxxxxxx

# === FAZ-Y: Upstash Redis (Semantic Cache) ===
# Get from: https://console.upstash.com
REDIS_URL=redis://default:password@endpoint.upstash.io:6379

# === FAZ-Y: Bypass Flags (for gradual rollout) ===
BYPASS_GEMINI_EMBEDDINGS=false
BYPASS_SEMANTIC_CACHE=false
BYPASS_VECTOR_SEARCH=false

# === Other Services ===
SERPER_API_KEY=
FLUX_API_URL=http://localhost:7860/sdapi/v1/txt2img

# === Access Control ===
INTERNAL_ONLY=false
INTERNAL_WHITELIST_USER_IDS=

# === Session Secret ===
ATLAS_SESSION_SECRET=your_secret_key_here
